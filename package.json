{
  "name": "stonks-tradegate",
  "version": "1.0.0",
  "description": "Stocks and Technical Indicators using Tradegate Exchange",
  "main": "index.js",
  "scripts": {
    "test:unit": "nyc --reporter=lcov --reporter=text mocha test/unit/*",
    "test:integration": "mocha test/integration/*",
    "test": "npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgruendel/stonks-tradegate.git"
  },
  "keywords": [
    "stocks",
    "tradegate",
    "tradegatexchange"
  ],
  "author": "Stefan Gr√ºndel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sgruendel/stonks-tradegate/issues"
  },
  "homepage": "https://github.com/sgruendel/stonks-tradegate#readme",
  "dependencies": {
    "moment": "2.29.4",
    "mongoose": "6.10.0",
    "node-fetch": "2.6.9",
    "trading-signals": "3.6.1",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "chai": "4.3.7",
    "eslint": "8.34.0",
    "eslint-config-strongloop": "2.1.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": "strongloop",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "error",
        120,
        4,
        {
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignorePattern": "^\\s*var\\s.+=\\s*(require\\s*\\()|(/)"
        }
      ]
    }
  }
}
