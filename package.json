{
    "name": "stonks-tradegate",
    "version": "1.0.0",
    "description": "Stocks and Technical Indicators using Tradegate Exchange",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test:unit": "c8 --reporter=lcov --reporter=text mocha test/unit/*",
        "test:integration": "mocha test/integration/*",
        "test": "npm run test:unit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sgruendel/stonks-tradegate.git"
    },
    "keywords": [
        "stocks",
        "tradegate",
        "tradegateexchange"
    ],
    "author": "Stefan Gr√ºndel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sgruendel/stonks-tradegate/issues"
    },
    "homepage": "https://github.com/sgruendel/stonks-tradegate#readme",
    "dependencies": {
        "moment": "^2.30.1",
        "mongoose": "^8.4.1",
        "node-fetch": "^3.3.2",
        "p-map": "^7.0.2",
        "trading-signals": "^4.0.0",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "c8": "^9.1.0",
        "chai": "^5.1.1",
        "eslint": "^9.4.0",
        "eslint-config-strongloop": "^2.1.0",
        "mocha": "^10.4.0"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 2023,
            "sourceType": "module"
        },
        "extends": "strongloop",
        "env": {
            "es6": true,
            "node": true,
            "mocha": true
        },
        "rules": {
            "indent": [
                "error",
                4
            ],
            "max-len": [
                "error",
                120,
                4,
                {
                    "ignoreComments": true,
                    "ignoreUrls": true,
                    "ignoreStrings": true,
                    "ignorePattern": "^\\s*var\\s.+=\\s*(require\\s*\\()|(/)"
                }
            ],
            "space-before-function-paren": 0
        }
    }
}
