{
    "name": "stonks-tradegate",
    "version": "1.0.0",
    "description": "Stocks and Technical Indicators using Tradegate Exchange",
    "keywords": [
        "stocks",
        "tradegate",
        "tradegateexchange"
    ],
    "homepage": "https://github.com/sgruendel/stonks-tradegate#readme",
    "bugs": {
        "url": "https://github.com/sgruendel/stonks-tradegate/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sgruendel/stonks-tradegate.git"
    },
    "license": "ISC",
    "author": "Stefan Gr√ºndel",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "npm run test:unit",
        "test:integration": "mocha test/integration/*",
        "test:unit": "c8 --reporter=lcov --reporter=text mocha test/unit/*"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "mocha": true,
            "node": true
        },
        "parserOptions": {
            "ecmaVersion": 2023,
            "sourceType": "module"
        },
        "extends": "strongloop",
        "rules": {
            "indent": [
                "error",
                4
            ],
            "max-len": [
                "error",
                120,
                4,
                {
                    "ignoreComments": true,
                    "ignoreUrls": true,
                    "ignoreStrings": true,
                    "ignorePattern": "^\\s*var\\s.+=\\s*(require\\s*\\()|(/)"
                }
            ],
            "space-before-function-paren": 0
        }
    },
    "dependencies": {
        "moment": "^2.30.1",
        "mongoose": "^8.9.2",
        "node-fetch": "^3.3.2",
        "p-map": "^7.0.3",
        "trading-signals": "^5.0.4",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "c8": "^10.1.3",
        "chai": "^5.1.2",
        "eslint": "^9.17.0",
        "eslint-config-strongloop": "^2.1.0",
        "mocha": "^11.0.1"
    }
}
